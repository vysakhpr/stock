<%= form_for @labid, :url=>"/labid/create" do |t| %>
    <div><%= render 'shared/error_messages', object: t.object %></div>

    <div><%= t.label :name %>
    	<%=t.text_field :name%> </div>

    <div><%= t.hidden_field :user_id,:value=>current_user.id %></div>

    <div><%= t.submit "Add"%></div>
 <%end%>